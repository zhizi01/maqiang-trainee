{
  "name": "maqiang-trainee",
  "description": "马枪练习生 - 是一款免费开源的枪法练习软件，内嵌CSGO一键导入练习地图及启动。",
  "version": "2023.11.15",
  "author": "maqiang-trainee",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^3.8.3",
    "element-plus": "^2.4.2",
    "vue": "^3.2.13"
  },
  "build": {
    "appId": "site.nb8.mq",
    "productName": "马枪练习生",
    "directories": {
      "output": "builder"
    },
    "win": {
      "icon": "src/assets/favicon/favicon.ico",
      "target": [
        "nsis"
      ]
    }
  },
  "main": "main.js",
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "electron": "^22.3.9",
    "electron-builder": "^24.6.4",
    "electron-reloader": "^1.2.3",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
